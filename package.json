{
  "name": "dad",
  "version": "",
  "description": "Deployment Analytics Dashboard",
  "scripts": {
    "start": "gulp",
    "dist": "cross-env NODE_ENV=production gulp",
    "test": "jest --verbose && govendor test +local",
    "test:client": "jest --verbose",
    "test:server": "govendor test +local",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --verbose --watch",
    "lint:client": "eslint --ext .js --ext .jsx '*.js' client/src && sass-lint 'client/src/**/*.scss'",
    "lint:server": "golangci-lint run --config=golangci-linter.json ./server/... ./cmd/... .",
    "fix:client": "eslint --fix 'client/src/**/*.js'"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(s?css)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "client/src/**/*.{js,jsx}"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/soprasteria/dad.git"
  },
  "private": true,
